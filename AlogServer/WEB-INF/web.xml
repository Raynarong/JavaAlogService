<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <servlet>
  <description>This is the description of my J2EE component</description>
  <display-name>This is the display name of my J2EE component</display-name>
  <servlet-name>HelloServlet</servlet-name>
  <servlet-class>RpcService.HelloServlet</servlet-class>
 </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HelloworldServlet</servlet-name>
    <servlet-class>RpcService.HelloworldServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReceiveData</servlet-name>
    <servlet-class>Load.ReceiveData</servlet-class>
  </servlet>
  
   <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReceiveFileData</servlet-name>
    <servlet-class>Load.ReceiveFileData</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SaveData</servlet-name>
    <servlet-class>Load.SaveData</servlet-class>
  </servlet>
<servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReceiveJar</servlet-name>
    <servlet-class>Load.ReceiveJar</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ReturnSource</servlet-name>
    <servlet-class>Load.ReturnSource</servlet-class>
  </servlet>


 <servlet-mapping>
  <servlet-name>HelloServlet</servlet-name>
  <url-pattern>/servlet/HelloServlet</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HelloworldServlet</servlet-name>
    <url-pattern>/servlet/HelloworldServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReceiveData</servlet-name>
    <url-pattern>/ReceiveData</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
    <servlet-name>ReceiveFileData</servlet-name>
    <url-pattern>/ReceiveFileData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaveData</servlet-name>
    <url-pattern>/SaveData</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ReturnSource</servlet-name>
    <url-pattern>/ReturnSource</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ReceiveJar</servlet-name>
    <url-pattern>/ReceiveJar</url-pattern>
  </servlet-mapping>
  
   
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
